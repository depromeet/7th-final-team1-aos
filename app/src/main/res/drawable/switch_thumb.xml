        android:right="1dp"
        android:top="1dp">

        <shape android:shape="oval">

            <size
                android:width="29dp"
                android:height="29dp" />

            <solid android:color="#FFFFFF" />

            <stroke
                android:width="1dp"
                android:color="#FFFFFF" />

        </shape>
    </item>
</layer-list>